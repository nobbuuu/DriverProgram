<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape android:shape="line">
            <stroke android:width="@dimen/dp_1" android:color="@color/input_edittext_line_unselect_color" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="line">
            <stroke android:width="@dimen/dp_1" android:color="@color/maas_main" />

        </shape>
    </item>

    <item>
        <shape android:shape="line">
            <stroke android:width="@dimen/dp_1" android:color="@color/input_edittext_line_unselect_color" />
        </shape>
    </item>

</selector>